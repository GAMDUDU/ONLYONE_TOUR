<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    


<mapper namespace="com.onlyone.model.MemberDAO">

	<select id="login" parameterType="com.onlyone.model.MemberDTO">
		
		select member_name from Member_trip
		where member_id = #{member_id} and member_pwd = #{member_pwd}
		
	</select>

</mapper>
